<body class="login-body">
  <div class="form-signin">
    <div class=" ">
      <header class="">
      </header>
      <div id="kc-content">
        <div id="kc-content-wrapper">
        <div id="kc-form" class="">
            <div id="kc-form-wrapper" class=" ">
                    <form id="kc-form-login" onsubmit="login.disabled = true; return true;" action="https://sso.cloud.pje.jus.br/auth/realms/pje/login-actions/authenticate?session_code=QqcrOoB6e4aKHGwmsaWXkWCC2P_1VyadiVY3V28g7SI&amp;execution=a004f348-bcca-4987-b5f7-dd119569fbfc&amp;client_id=pje-tjmg-1g&amp;tab_id=5UNu8N4BSts" method="post">
                        <div class="container">
                            <div class="row align-items-center form-wrapper">
                                <div class="col-md-6">
                                    <input type="button" class="btn botao-certificado" name="login-pje-office" id="kc-pje-office" value="CERTIFICADO DIGITAL" onclick="autenticar('');"/>
                                </div>
                                <div class="col-md-6 div-login">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                                <input tabindex="1" id="username" class="form-control" name="username" placeholder="CPF/CNPJ" type="text" autofocus autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <input tabindex="2" id="password" class="form-control" name="password" placeholder="Senha" type="password" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="col-md-6 float-right">
                                        <div id="kc-form-buttons" class="">
                                            <input tabindex="4" class="btn btn-primary btn-block botao-entrar" name="login" id="kc-login" type="submit" value="Entrar"/>
                                            <input type="hidden" id="pjeoffice-code" name="pjeoffice-code" value="">
                                            <input type="hidden" id="phrase" name="phrase" value="">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
            </div>
        </div>


              <div id="kc-info" class="">
                  <div id="kc-info-wrapper" class="">

                  </div>
              </div>
        </div>
      </div>

    </div>
  </div>




